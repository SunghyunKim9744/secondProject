<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.ggiriggiri.web.dao.ChatDao">
	<select id="getList" resultType="com.ggiriggiri.web.entity.ChatRoom">
		select * from ChatRoom
	</select>
	
	<select id="get" resultType="com.ggiriggiri.web.entity.ChatRoom">
		select * from ChatRoom where id=${id}
	</select>
</mapper>