<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.ggiriggiri.web.dao.StudySkillDao">
  <select id="getViewList" resultType="com.ggiriggiri.web.entity.StudySkillView">
  		select * from StudySkillView where studyId=#{id}
     
   </select>
   
   <select id="getByStudyIds" resultType="int">
   	select studyId from StudySkillView where skillName in 
	<foreach item="sk" index="index" collection="skill"
   			open="(" separator="," close=")">
   			#{sk}
   		</foreach>
 group by studyId; 
   </select>
    
    
    </mapper>